pipeline {
 agent none
 stages
 {
  stage('Checkout')
  {
  agent { label 'demo' }
   steps { 
    git branch: 'master', url: 'https://gitlab.com/scmlearningcentre/WEZVACICD.git'
   }
  }

  stage('PRECHECK')
  {
   agent { label 'demo' }
    when { changeset "samplejar/*"}
    steps {
       echo "IT WORKED"
    }
  }

  
 } //End of Stages

} //End of Pipeline

